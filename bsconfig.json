{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": [{
    "dir" : "src",
    "subdirs" : true
  }, {
    "dir": "__tests__",
    "subdirs": true
  }],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@glennsl/bs-jest"
  ],
  "warnings": {
    "error" : "+101"
  },
}
